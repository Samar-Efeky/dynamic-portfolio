{
  "version": 2,
  "builds": [
    {
      "src": "dist/dynamic-portfolio/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/dynamic-portfolio/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "dist/dynamic-portfolio/server/main.js"
    }
  ]
}
